{
  "name": "PPH",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build",
    "prev": "serve -s dist",
    "start": "node server.js",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve --port 8080",
    "copyWasm": "copy-files-from-to --config copy-files-from-to.json",
    "dev": "npm run copyWasm && vue-cli-service serve --port 8080",
    "build": "npm run copyWasm && vue-cli-service build"
  },
  "engines": {
    "node": "18.17.0",
    "npm": "9.8.1"
  },
  "dependencies": {
    "@canvasjs/charts": "^3.7.33",
    "@canvasjs/vue-charts": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.8",
    "@quasar/extras": "^1.0.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "apexcharts": "^3.45.1",
    "chart.js": "^4.4.1",
    "core-js": "^3.8.3",
    "epic-spinners": "^2.0.0",
    "express": "^4.18.2",
    "jwt-decode": "^3.1.2",
    "mime-types": "^2.1.35",
    "nprogress": "^0.2.0",
    "particles.js": "^2.0.0",
    "quasar": "^2.0.0",
    "register-service-worker": "^1.7.2",
    "scichart": "^3.2.549",
    "three": "^0.156.0",
    "vee-validate": "^4.5.8",
    "vue": "^3.2.13",
    "vue-apexcharts": "^1.6.2",
    "vue-chartjs": "^5.3.0",
    "vue-particlejs": "^1.1.0",
    "vue-router": "^4.0.3",
    "vue-toast-notification": "^3.1.1",
    "vue3-apexcharts": "^1.4.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.22.10",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "copy-files-from-to": "^3.2.1",
    "css-loader": "^6.8.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1",
    "gh-pages": "^6.1.1",
    "sass": "1.32.12",
    "sass-loader": "^12.0.0",
    "vue-cli-plugin-quasar": "~5.0.2",
    "vue-eslint-parser": "^9.3.1",
    "vue-style-loader": "^4.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "vue/no-unused-vars": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
